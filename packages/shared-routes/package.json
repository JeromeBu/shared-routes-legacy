{
  "name": "shared-routes",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.3.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "lint": "eslint src --fix",
    "format:write": "pnpm -w format-cmd --write ./packages/shared-routes/src/",
    "format:check": "pnpm -w format-cmd --check ./packages/shared-routes/src/",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "config": "workspace:*",
    "eslint": "^8.11.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "zod": "^3.11.6"
  }
}
