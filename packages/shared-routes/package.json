{
  "name": "shared-routes",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.4.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && pnpm -w tsc-cmd -p ./packages/shared-routes/tsconfig.build.json",
    "watch": "pnpm -w tsc-cmd -p ./packages/shared-routes/tsconfig.build.json --watch",
    "lint": "eslint src --fix",
    "format": "pnpm -w format-cmd --write ./packages/shared-routes/src/",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "config": "workspace:*",
    "eslint": "^8.11.0",
    "tsconfig": "workspace:*"
  },
  "dependencies": {
    "zod": "^3.11.6"
  }
}
